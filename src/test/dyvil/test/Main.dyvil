package test;

import dyvil.lang.annotation.Bytecode;
import dyvil.lang.annotation.byref;
import dyvil.reflect.Opcodes;
import java.util.Arrays;

public class Main
{
	public static void main(String[] args) =
	{
		args(0) = "Test";
		args print;
		System out println(test()(1));
	};
	
	public implicit void print(int[] a) = System out println(Arrays toString(a));
	
	public implicit void print(float[] a) = System out println(Arrays toString(a));
	
	public implicit void print(String[] a) = System out println(Arrays toString(a));
	
	public implicit void print(int i) = System out println(i);
	
	public static String[] test() = { "Hello", "World" };
}
