package dyvil.test

public class AbstractPropertyTest
{
	interface AbstractProperty {
		public int myProp {
			abstract get
			abstract set
		}
	}
	
	class ConcreteProperty implements AbstractProperty
	{
		private int value

		public override int myProp
		{
			set(newValue) {
				this.value = newValue
			}

			get {
				return this.value
			}
		}
	}
	
	public static void main([String] args)
	{
		auto c = new ConcreteProperty

		c.myProp = 20
		Main.test("Property Getter", 20, c.myProp)

		auto result = c.myProp = 10
		Main.test("Property Setter", 10, result)
	}
}
