package dyvil.test

public class AbstractProperties
{
	interface A {
		public int myProp {
			abstract get
			abstract set
		}
	}
	
	class C implements A
	{
	    public override int myProp {
	    	set : {}
	    	get : 1
	    }
	}
	
	public static void main([String] args)
	{
		auto c = new C
		Main.test("Property Getter", 1, c.myProp)

		auto result = c.myProp = 10
		Main.test("Property Setter", 10, result)
	}
}
