package dyvil.test

public class RangeTest
{
	public static void main([String] args)
	{
		testInts()
		testOrdered()
	}
	
	private static void testInts()
	{
		auto range1 = 1 .. 4
		auto range2 = 1 ..< 4

		List[int] rangeInclusive = List.[int]
		for (auto i : range1) {
			rangeInclusive += i
		}
		Main.test("Int Range (inclusive)", List(1, 2, 3, 4), rangeInclusive)

		List[int] rangeExclusive = List.[int]
		for (auto i : range2) {
			rangeExclusive += i
		}
		Main.test("Int Range (exclusive)", List(1, 2, 3), rangeExclusive)

		List[int] forInclusive = List.[int]
		for (auto i : 1 .. 4) {
			forInclusive += i
		}
		Main.test("Int Range For (inclusive)", List(1, 2, 3, 4), forInclusive)

		List[int] forExclusive = List.[int]
		for (auto i : 1 ..< 4) {
			forExclusive += i
		}
		Main.test("Int Range For (exclusive)", List(1, 2, 3), forExclusive)
	}
	
