package dyvil.tests

using dyvil.Test

class StringTest
{
	static func apply() -> Test = test "Strings" {
		test "String Interpolation" {
			let ab = 'a' ++ 'b'
			ab =?= "ab"

			let abbccd = 'a' + "bb" ++ "cc" + 'd'
			abbccd =?= "abbccd"

			let abbcdde = ('a' + "bb" + 'c') ++ "dd" + 'e'
			abbcdde =?= "abbcdde"
		}
	}
}
