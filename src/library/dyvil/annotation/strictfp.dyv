package dyvil.annotation

import java.lang.annotation._

/**
 * Substitute annotation for the {@code strictfp} keyword in Java. The <i>Dyvil Compiler</i> directly converts this
 * annotation to the {@code ACC_STRICT} flag.
 */
@Target([ ElementType.METHOD, ElementType.TYPE, ElementType.CONSTRUCTOR ])
@Retention(RetentionPolicy.SOURCE)
@interface strictfp
