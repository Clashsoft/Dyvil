	@DyvilModifiers(Modifiers.INFIX)
	public static PRIMITIVE @NonNull [] flatMapped(PRIMITIVE @NonNull [] array, TO_MAPPER PRIMITIVE @NonNull [] TO_MAPPER_END mapper)
	{
		int size = 0;
		PRIMITIVE @NonNull [] res = new PRIMITIVE[array.length << 2];

		for (PRIMITIVE v : array)
		{
			final PRIMITIVE @NonNull [] a = mapper.TO_MAPPER_APPLY(v);
			final int newSize = a.length;

			if (size + newSize >= res.length)
			{
				res = Arrays.copyOf(res, (size + newSize) << 1);
			}

			System.arraycopy(a, 0, res, size, newSize);
			size += newSize;
		}

		if (size == res.length)
		{
			return res;
		}
		return Arrays.copyOf(res, size);
	}

	@DyvilModifiers(Modifiers.INFIX)
