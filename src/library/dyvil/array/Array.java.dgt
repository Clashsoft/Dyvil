	@DyvilModifiers(Modifiers.INFIX)
	public static int indexOf(PRIMITIVE @NonNull [] array, PRIMITIVE value, int startIndex)
	{
		for (; startIndex < array.length; startIndex++)
		{
			if (array[startIndex] == value)
			{
				return startIndex;
			}
		}
		return -1;
	}

