	@DyvilModifiers(Modifiers.INFIX)
	public static int lastIndexOf(PRIMITIVE @NonNull [] array, PRIMITIVE value, int startIndex)
	{
		for (; startIndex >= 0; startIndex--)
		{
			if (array[startIndex] == value)
			{
				return startIndex;
			}
		}
		return -1;
	}

