	@DyvilModifiers(Modifiers.INFIX)
	public static String toString(PRIMITIVE @NonNull [] array)
	{
		if (array == null)
		{
			return "null";
		}

		final int size = array.length;
		if (size == 0)
		{
			return "[]";
		}

		final StringBuilder builder = new StringBuilder();
		append(array, size, builder);
		return builder.toString();
	}

	@DyvilModifiers(Modifiers.INFIX)
	public static void toString(PRIMITIVE @NonNull [] array, @NonNull StringBuilder builder)
	{
		if (array == null)
		{
			builder.append("null");
			return;
		}

		final int size = array.length;
		if (size == 0)
		{
			builder.append("[]");
			return;
		}

		append(array, size, builder);
	}

	private static void append(PRIMITIVE @NonNull [] array, int size, @NonNull StringBuilder builder)
	{
		builder.append('[').append(array[0]);
		for (int i = 1; i < size; i++)
		{
			builder.append(", ");
			builder.append(array[i]);
		}
		builder.append(']');
	}
