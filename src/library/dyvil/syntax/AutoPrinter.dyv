package dyvil.syntax

object AutoPrinter
{
	static func print(closure: AutoPrinter.() -> void) -> void inline = closure(AutoPrinter)

	func applyStatement(value: any!) -> void = print value
}

@BytecodeName("AutoPrinter$OutputSyntax")
extension Output
{
	static func print(closure: AutoPrinter.() -> void) -> void inline = closure(AutoPrinter)
}
