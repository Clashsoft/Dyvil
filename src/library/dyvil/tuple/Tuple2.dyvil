package dyvil.tuple

import dyvil.lang.Entry

public case class Tuple2[+A, +B] (
		public final A _1
		public final B _2)
		implements Entry[A, B]
{
	public override String toString() = @"(\(this._1), \(this._2))"
			
	public override A getKey() = this._1
	
	public override B getValue() = this._2
	
	public override int hashCode() = Entry entryHashCode this
}